function initComponent(){$("#inputHiddenProjectCnt").text()=="0"&&$("#tblProject tbody tr").remove();$("#btnNewProject").on("click",function(){$("#divNewProject").show()});$("#btnNewProjectCancel").on("click",function(){$("#projectForm").validationEngine("hideAll");$("#divNewProject").hide()});$("#hrefNewProjectPanelClose").on("click",function(){$("#divNewProject").hide()});formatDataTable();$("#txtNewProjectStartDate").datetimepicker({showButtonPanel:!1,showTime:!1,showHour:!1,showMinute:!1})}function buildDeleteBtn(e,t,n){$("#"+e).on("click",function(){$.prompt("Are you sure want to delete project: "+n+" ?",{title:"Confirm Deletion",buttons:{Delete:!0,No:!1},top:"40%",submit:function(e,n,r,i){n&&$.ajax({type:"DELETE",url:$("#hiddenRoot").text()+"/rest/project/delete/"+t}).done(function(e){}).fail(function(){}).always(function(){location.reload()})}})})}function confirmDelete(e,t,n){e.status=="success"&&$.prompt("Are you sure want to delete project: "+n+" ?",{title:"Confirm Deletion",buttons:{Delete:!0,No:!1},top:"40%",submit:function(e,n,r,i){if(n){$("#deleteProjectIdHidden").val(t);$("#btnDeleteProjectHidden").click()}}})}function handleFormError(e){alert(e.description)}function formatDataTable(){projectTable=$("#tblProject").dataTable({bJQueryUI:!0,bAutoWidth:!1,sPaginationType:"full_numbers",sDom:'<""f>t<"F"lp>'});$("select").uniform()}function createProjectEventHandler(e){if(e.status=="begin")projectTable.fnDestroy();else if(e.status=="success"){$("#txtNewProjectStartDate").datetimepicker({showButtonPanel:!1,showTime:!1,showHour:!1,showMinute:!1});$("#btnNewProjectCancel").on("click",function(){$("#projectForm").validationEngine("hideAll");$("#divNewProject").hide()});$("#hrefNewProjectPanelClose").on("click",function(){$("#divNewProject").hide()});$("#inputHiddenProjectCnt").text()=="0"&&$("#tblProject tbody tr").remove();formatDataTable()}else e.status=="complete"}function deleteProjectEventHandler(e){if(e.status=="begin"){console.log("Delete begin");projectTable.fnDestroy()}else if(e.status=="success"){console.log("Delete success");$("#inputHiddenProjectUpdatedCnt").val()=="0"&&$("#tblProject tbody tr").remove();formatDataTable()}else e.status=="complete"&&console.log("Delete completed")}var projectTable;