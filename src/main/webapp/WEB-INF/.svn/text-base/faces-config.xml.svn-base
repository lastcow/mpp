<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>dashboard</from-outcome>
   <to-view-id>/modules/main.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>project</from-outcome>
   <to-view-id>/modules/project.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>schedule</from-outcome>
   <to-view-id>/modules/schedule.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>resource</from-outcome>
   <to-view-id>/modules/resource.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>help</from-outcome>
   <to-view-id>/modules/help.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/projectportal.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>projectdetail</from-outcome>
   <to-view-id>/modules/projectdetail.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- <navigation-case>
   <from-action>#{identity.login}</from-action>
   <if>#{identity.loggedIn}</if>
   <to-view-id>/modules/main.xhtml</to-view-id>
   <redirect/>
  </navigation-case> -->
  <navigation-case>
   <from-action>#{identity.logout}</from-action>
   <if>#{true}</if>
   <to-view-id>/projectportal.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>profile</from-outcome>
   <to-view-id>/modules/profile.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>tasks</from-outcome>
   <to-view-id>/modules/tasks.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>settings</from-outcome>
   <to-view-id>/modules/settings/dashboard.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>settings-calendar</from-outcome>
   <to-view-id>/modules/settings/calendar.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>settings-project</from-outcome>
   <to-view-id>/modules/settings/project.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <locale-config>
   <default-locale>jp</default-locale>
   <supported-locale>en</supported-locale>
   <supported-locale>zh</supported-locale>
   <supported-locale>jp</supported-locale>
  </locale-config>
  <resource-bundle>
   <base-name>strings.projStrings</base-name>
   <var>projStrings</var>
  </resource-bundle>
  <resource-bundle>
   <base-name>strings.validateStrings</base-name>
   <var>validate</var>
  </resource-bundle>
 </application>
</faces-config>
